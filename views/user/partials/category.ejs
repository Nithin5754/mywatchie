

  <div class="flex flex-row justify-around items-center flex-wrap  gap-5">
  
<%  randomCategory.forEach((element,index) => { %>
    
  <div
    class="flex flex-col justify-around items-center p-1  rounded-lg bg-gray-200 border shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] dark:bg-neutral-700 w-60 h-80">
    <div class="relative overflow-hidden bg-cover bg-no-repeat">
      <% if(isLogin) { %>
        <a href="/productList/<%= element.product_category%>"><img class="rounded-t-lg w-48" src="<%= element.category_img_url %>" alt="" /></a>
     <% }else{%>
<a href="/login"><img class="rounded-t-lg w-48"
    src="<%= element.category_img_url %>" alt="" /></a>
     <%}%>
    </div>
    <div class="bg-gray-200 ">
      <h1 class="text-center font-extrabold">
      <%= element.product_category%>
      </h1>
    </div>
  </div>
 
   
<% }); %>









</div> 












